{
  "name": "gdmn-front",
  "description": "gdmn web client",
  "scripts": {
    "--- gdmn ---": "",
    "build": "yarn run pkg:build",
    "clean": "shx rm -rf ./dist ./build",
    "rebuild": "yarn run clean && yarn run build",
    "start": "yarn run pkg:start",
    "watch": "tsc -w",
    "------------": "",
    "ci:compile": "yarn run clean && tsc -p ./tsconfig.json",
    "pkg-links:build": "lerna run rebuild --scope={$(ls ../../packages | xargs echo | sed 's/ /,/g')}",
    "pkg:build": "cross-env TS_NODE_PROJECT=configs/tsconfig.webpack.json webpack --progress --config configs/webpack/webpack.config.prod.ts",
    "pkg:deploy": "shx rm -rf ../gdmn-back/public && shx cp -r ./dist ../gdmn-back/public",
    "pkg:start": "cross-env TS_NODE_PROJECT=configs/tsconfig.webpack.json webpack-dev-server --config configs/webpack/webpack.config.dev.ts",
    "prettier:cli": "prettier \"**/*.@(ts|tsx|js|jsx)\"",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "prettier:lint": "yarn run prettier:cli -- -l",
    "yarn:clean": "cd ../../ && shx rm -rf yarn.lock && lerna exec -- shx rm -rf node_modules",
    "yarn:reinstall": "yarn run yarn:clean && yarn"
  },
  "dependencies": {
    "@stomp/stompjs": "5.0.1",
    "es6-error": "4.1.1",
    "gdmn-grid": "1.0.0",
    "gdmn-nlp": "1.0.0",
    "gdmn-nlp-agent": "1.0.0",
    "gdmn-orm": "1.0.0",
    "gdmn-recordset": "1.0.0",
    "immutable": "^3.8.2",
    "intl": "1.2.5",
    "intl-format-cache": "2.1.0",
    "jwt-decode": "2.2.0",
    "office-ui-fabric-react": "^6",
    "react": "16.6.3",
    "react-content-loader": "^3.1.2",
    "react-css-modules": "4.7.8",
    "react-dom": "16.6.3",
    "react-redux": "^5.1.1",
    "react-router-breadcrumbs-hoc": "^2.1.5",
    "react-router-dom": "^4.3.1",
    "recompose": "0.30.0",
    "redux": "^4.0.1",
    "redux-persist": "5.10.0",
    "rxjs": "6.3.3",
    "typesafe-actions": "2.0.4"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "0.1.2",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/intl": "1.2.0",
    "@types/jest": "^23.3.10",
    "@types/jwt-decode": "2.2.1",
    "@types/mini-css-extract-plugin": "0.2.0",
    "@types/react": "16.7.13",
    "@types/react-content-loader": "3.1.3",
    "@types/react-css-modules": "4.6.2",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/recompose": "0.30.0",
    "@types/redux-logger": "3.0.6",
    "@types/uglifyjs-webpack-plugin": "1.1.0",
    "@types/webpack": "^4.4.20",
    "@types/webpack-bundle-analyzer": "2.13.0",
    "@types/webpack-dev-server": "3.1.1",
    "@types/webpack-merge": "4.1.3",
    "awesome-typescript-loader": "5.2.1",
    "clean-webpack-plugin": "^1",
    "cross-env": "5.2.0",
    "css-loader": "^1.0.0",
    "error-overlay-webpack-plugin": "0.1.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "^0.4.1",
    "prettier": "^1.15.2",
    "react-hot-loader": "4.5.2",
    "redux-logger": "^3.0.6",
    "shx": "0.3.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "1.1.0",
    "ts-loader": "5.3.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "private": true,
  "version": "1.0.0-alpha.1"
}
